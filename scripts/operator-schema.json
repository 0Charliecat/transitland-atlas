{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Transitland Atlas operator schema",
  "type": "object",
  "properties": {
    "onestop_id": {
      "type": "string"
    },
    "tags": {
      "type": "object",
      "properties": {
        "us_ntd_id": {
          "type": ["string", "array"]
        },
        "omd_provider_id": {
          "type": ["string", "array"]
        },
        "wikidata_id": {
          "type": ["string", "array"]
        }
      }
    },
    "name": {
      "type": "string"
    },
    "short_name": {
      "type": ["string", "null"]
    },
    "website": {
      "type": ["string", "null"]
    },
    "associated_feeds": {
      "type": "array"
    }
  }
}